FILE_TYPE = FORM_DEFN VERSION=2      
FORM
FIXED
PORT 40 30
HEADER "自动创建Gnd过孔"
TILE

TEXT "单位"
TLOC 2 2
ENDTEXT
POPUP <unitsel_pop> 	"" ""
FIELD unitsel
FLOC 10 2
ENUMSET 4
POP "unitsel_pop"
ENDFIELD

TEXT "选择过孔:"
TLOC 2 5
ENDTEXT
POPUP <viasel_pop> 	"" ""
FIELD viasel
FLOC 10 5
ENUMSET 7
POP "viasel_pop"
ENDFIELD


TEXT "摆放位置:"
TLOC 2 8
ENDTEXT
POPUP <placetype_pop> 	"" ""
FIELD placetype
FLOC 10 8
ENUMSET 13
POP "placetype_pop"
ENDFIELD

TEXT "孔与线、焊盘安全间距:"
TLOC 2 11
ENDTEXT
FIELD via_svalue
FLOC 20 11
REALFILLIN 4 4
ENDFIELD

TEXT "孔间距"
TLOC 2 14
ENDTEXT
FIELD via_space
FLOC 20 14
REALFILLIN 4 5
ENDFIELD

TEXT "GND信号名"
TLOC 2 17
ENDTEXT
POPUP <gndname_pop> 	"" ""
FIELD gndname
FLOC 13 17
ENUMSET 10
POP "gndname_pop"
ENDFIELD

FIELD delvia
FLOC 2 21
MENUBUTTON "返回" 7 4
ENDFIELD
FIELD addvias
FLOC 16 21
MENUBUTTON "创建" 9 4
ENDFIELD


ENDTILE
ENDFORM
